<head>
    
    <link rel="icon" type="image/png" href="/img/ONLEX-LOGO.png">
    <title>ONLEX | QUESTION BANK</title>
    <script src="https://kit.fontawesome.com/fc494360f4.js" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <!-- <link rel="stylesheet" href="/styles/style.css"> -->
    <link rel="stylesheet" href="/styles/question.css">
    <link rel="stylesheet" href="/styles/registration.css">
</head>
<body>
    
<%-include("./includes/subAdminHeader")%>
<div class="table-container">
<table id="table">
    <thead>
        <tr>
            <%for(var item of head){%>
                <%if(item=="Intitule"){%>
            <th style="width:400px"><%=item%></th>
            <%}else{%>
            <th><%=item%></th>
            <%}%>
            <%}%>
            <th>delete</th>
            <th>update</th>
        </tr>
        
    </thead>
    <tbody>
        
            <%for (var list of data) {%>
                <tr>
                <%for (var item in list) {%>
                <td><%=list[item]%></td>
                <%}%>
                <td><a href="/question/deletequestion/?iqid=<%=list[Object.keys(list)[0]]%>"><i class="fas fa-trash-alt hot-pink-text"></i></a></td>
                <td><a href="/question/updatequestion/?iqid=<%=list[Object.keys(list)[0]]%>"><i class="far fa-edit green-text"></i></a></td>
                
                </tr>
                <%}%>
        
    </tbody>
</table>
</div>
</body>
